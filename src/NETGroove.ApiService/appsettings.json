        }
      },
      "route6": {
        "ClusterId": "cluster6",
        "Match": {
          "Path": "/api/user/{**catch-all}"
        }
      },
      "route7": {
        "ClusterId": "cluster7",
        "Match": {
          "Path": "/{**catch-all}"
        }
      }
